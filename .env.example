# Project directory paths (relative to harbourX folder)
PROJECT_ROOT=..
BACKEND_DIR=HarbourX-Backend
FRONTEND_DIR=HarbourX-Frontend
AI_MODULE_DIR=AI-Module
HARBOURX_DIR=harbourx

# Database Configuration (REQUIRED for production)
POSTGRES_DB=harbourx
POSTGRES_USER=harbourx
POSTGRES_PASSWORD=CHANGE_THIS_PASSWORD_IN_PRODUCTION
DB_PORT=5432

# JWT Secret (REQUIRED - must be at least 256 bits)
# Generate a secure random string: openssl rand -base64 32
JWT_SECRET=CHANGE_THIS_JWT_SECRET_IN_PRODUCTION

# Frontend Allowed Origins (comma-separated)
FRONTEND_ALLOWED_ORIGINS=http://localhost:3001,http://localhost:80,http://frontend:80
